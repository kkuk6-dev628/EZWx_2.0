{
  "name": "nest-typescript-starter",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "nest": "nest",
    "next": "cd src/client && next",
    "prebuild": "rimraf dist",
    "build": "SET NODE_ENV=production nest build && cd src/client && next build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch --preserveWatchOutput",
    "start:debug": "nest start --debug --watch --preserveWatchOutput",
    "start:prod": "SET NODE_ENV=production node dist/src/server/main",
    "lint": "eslint \"{src,test}/**/*.{ts,tsx}\"",
    "test": "jest --setupFiles dotenv-flow/config --collect-coverage",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:request": "jest --setupFiles dotenv-flow/config --config ./test/jest-request.json",
    "console": "ts-node -r tsconfig-paths/register src/server/console.ts",
    "typeorm": "typeorm-ts-node-commonjs -d ormconfig.ts",
    "client:generate:schema": "zeus src/schema.gql src/client/app/types --typescript --apollo",
    "cy": "cypress"
  },
  "dependencies": {
    "@apollo/client": "^3.7.2",
    "@apollo/gateway": "^2.2.2",
    "@babel/runtime": "^7.20.6",
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@material-ui/core": "latest",
    "@material-ui/icons": "latest",
    "@mui/material": "^5.11.1",
    "@nestjs/apollo": "^10.1.7",
    "@nestjs/common": "^9.2.1",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.2.1",
    "@nestjs/graphql": "^10.1.7",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.2.1",
    "@nestjs/typeorm": "^9.0.1",
    "@tippyjs/react": "^4.2.6",
    "@turf/boolean-point-in-polygon": "^6.5.0",
    "apollo-server-express": "^3.11.1",
    "axios": "^1.2.1",
    "commander": "^9.4.1",
    "cookie-parser": "^1.4.6",
    "geojson-to-svg": "^1.1.4",
    "google-auth-library": "^8.7.0",
