    "leaflet.markercluster": "^1.5.3",
    "lodash.groupby": "^4.6.0",
    "nestjs-console": "^8.0.0",
    "next": "^13.1.0",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-oauth2": "^1.6.1",
    "pg": "^8.8.0",
    "rc-slider": "^10.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.0",
    "react-icons": "^4.7.1",
