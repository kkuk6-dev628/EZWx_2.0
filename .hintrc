{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off"
      }
    ],
    "axe/text-alternatives": "off",
    "no-inline-styles": "off",
    "typescript-config/strict": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-ms-filter",
          "-webkit-overflow-scrolling"
        ]
      }
    ],
    "axe/parsing": [
      "default",
      {
        "duplicate-id-aria": "off"
      }
    ],
    "apple-touch-icons": "off",
    "axe/time-and-media": [
      "default",
      {
        "meta-refresh": "off"
      }
    ],
    "meta-viewport": "off"
  }
}